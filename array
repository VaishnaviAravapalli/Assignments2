#include <stdio.h>

int main()
{
   int a[10],b[10],i,j,n;
   printf("enter number of elements");
   scanf("%d",&n);
   printf("Enter the elements in array");
   for(i=0;i<n;i++)
   {
       scanf("%d",&a[i]);
   }
   for(i=0;i<n;i++)
   {
       b[i]=a[i];
   }
   printf("After copying the elements into another array");
   for(i=0;i<n;i++)
   {
       printf("%d",b[i]);
   }
}

